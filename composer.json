    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "zaporylie/composer-drupal-optimizations": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-drupal-scaffold-cmd": [
            "cd web && patch -p1 -r - <patches/htaccess-timbers.patch"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "patches": {
            "drupal/anchor_links": {
                "Fix pathing": "https://www.drupal.org/files/issues/2020-03-13/anchor_link-Wrong_path_to_libraries_in_current_profile-3119099-4.patch"
            },
            "drupal/core": {
                "Drupal alter's svg fill path's with base url -> broken svg": "https://www.drupal.org/files/issues/2021-11-04/drupal-9.3.x-fix_broken_svg-2362643-81.patch",
                "remove stuff": "https://www.drupal.org/files/issues/2020-01-10/2836539-34.patch"
            },
            "drupal/metatag": {
                "Error: Call to a member function __wakeup() on null": "https://www.drupal.org/files/issues/2022-01-13/metatag_views-fix-cache-wrapper-serialization-3258346-3.patch"
            },
            "drupal/scroll_to_destination_anchors": {
                "Allow scroll to work with current Drupal version": "https://www.drupal.org/files/issues/2018-04-18/stda-working_for_84-2916996-8_0.patch",
                "Automated Drupal 9 compatibility fixes": "https://www.drupal.org/files/issues/2020-06-06/scroll_to_destination_anchors.1.x-dev.rector.patch"
            },
            "drupal/select_icons": {
                "Automated Drupal 9 compatibility fixes": "https://www.drupal.org/files/issues/2020-09-09/3148725-4.patch"
            },
            "drupal/smart_date": {
                "PHP 8.1 Compatibility": "https://www.drupal.org/files/issues/2022-04-08/3269931-fixing-php8.1-compatibility-issue-2.patch"
            }
        },
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        }
    }
}
